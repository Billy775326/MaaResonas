{
	"DailyOrder": {
		"recognition": "OCR",
		"expected": "访问城市",
		"action": "Click",
		"timeout": 3000,
		"on_error": [
			"closeinfo@newsignincity@order",
			"home@Dailyorder"
		],
		"next": [
			"Dailyship"
		],

		"interrupt": [
			"inCity",
			"journey"
		]
	},

	"home@Dailyorder": {
		"recognition": "TemplateMatch",
		"template": "home.png",
		"action": "Click",
		"next": "DailyOrder"
	},

	"closeinfo@newsignincity@order": {
		"recognition": "OCR",
		"expected": [
			"触碰空白区域退出",
			"任务完成",
			"客运收益结算"
		],
		"action": "Click",
		"target": [
			100,
			661,
			105,
			19
		],
		"post_delay": 500,
		"next": [
			"closeinfo@newsignincity@order",
			"DailyOrder",
			"home@Dailyorder"
		]
	},

	"Dailyship": {
		"post_delay": 1500,
		"next": [
			"shop",
			"shopUpright",
			"shopDownleft"
		],
		"interrupt": [
			"home",
			"visitCity"
		]
	},

	"shipCity": {
		"recognition": "OCR",
		"doc": "带商会运输订单的城市",
		"expected": [
			"修格里城",
			"曼德矿场",
			"澄明数据中心",
			"7号自由港",
			"阿妮塔发射中心",
			"海角城",
			"岚心城",
			"贡露城"
		],
		"action": "Click",
		"target_offset": [
			15,
			0,
			0,
			0
		]
	},

	"shop": {
		"recognition": "OCR",
		"expected": "商会",
		"action": "Click",
		"target_offset": [
			0,
			50,
			0,
			0
		],
		"post_delay": 1000,
		"next": "shipOrder"
	},

	"shopUpright": {
		"next": [
			"shipOrder",
			"shop"
		],
		"interrupt": [
			"seekUpright"
		],
		"post_delay": 800
	},

	"shopDownleft": {
		"next": [
			"shipOrder",
			"shop"
		],
		"interrupt": [
			"seekDownleft"
		],
		"post_delay": 800
	},

	"shipConfirm1": {
		"recognition": "OCR",
		"doc": "商会运输订单再确认",
		"expected": [
			"接取订单"
		],
		"action": "Click"
	},

	"shipOrder": {
		"recognition": "OCR",
		"expected": [
			"运输订单"
		],
		"action": "Click",
		"post_delay": 1200,
		"next": "shipConfirm"
	},

	"shipConfirm": {
		"recognition": "OCR",
		"expected": [
			"接取订单",
			"查看订单"
		],
		"action": "Click",
		"post_delay": 600,
		"next": [
			"shipConfirmfinish",
			"nextOrder"
		],
		"interrupt": "shipConfirmfailed"
	},

	"shipConfirmfinish": {
		"recognition": "OCR",
		"expected": [
			"接取订单",
			"查看订单"
		],
		"action": "Click",
		"post_delay": 400,
		"next": [
			"shipConfirm",
			"nextOrder",
			"home"
		]
	},

	"nextOrder": {
		"recognition": "TemplateMatch",
		"template": "Order/next.png",
		"action": "Click",
		"post_delay": 200,
		"next": [
			"shipConfirm",
			"shipConfirmfailed"
		]
	},

	"shipConfirmfailed": {
		"recognition": "OCR",
		"expected": [
			"取消订单"
		],
		"doc": "home@tofight任务取消调用,使用plcli集成任务",
		"next": [
			"nextOrder",
			"home@StopTask"
		],
		"interrupt": "closeinfo@shipConfirmfailed"
	},

	"closeinfo@shipConfirmfailed": {
		"recognition": "OCR",
		"expected": [
			"触碰空白区域退出"
		],
		"action": "Click"
	},

	"home@tofight": {
		"recognition": "TemplateMatch",
		"template": "home.png",
		"action": "Click",
		"next": "DailyFight"
	},

	"visitCity": {
		"recognition": "OCR",
		"expected": "访问城市",
		"action": "Click"
	},

	"inCity": {
		"recognition": "OCR",
		"roi": [
			1135,
			605,
			145,
			115
		],
		"expected": "启程"
	},

	"home": {
		"recognition": "TemplateMatch",
		"template": "home.png",
		"action": "Click",
		"post_delay": 400,
		"doc": "回到首页"
	},

	"backup": {
		"recognition": "TemplateMatch",
		"template": "backup.png",
		"action": "Click",
		"doc": "返回上一级"
	},

	"seekUpright": {
		"action": "Swipe",
		"doc": "滑动屏幕寻找店铺右上方向",
		"begin": [
			630,
			322,
			10,
			10
		],
		"end": [
			842,
			454,
			10,
			10
		]
	},

	"seekDownleft": {
		"action": "Swipe",
		"doc": "滑动屏幕寻找店铺左下方向",
		"begin": [
			630,
			322,
			10,
			10
		],
		"end": [
			487,
			194,
			10,
			10
		]
	}
}
