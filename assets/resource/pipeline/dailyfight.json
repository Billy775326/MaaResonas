{
	"DailyFight": {
		"timeout": 3000,
		"on_error": [
			"closeinfo@newsignincity@fight",
			"home@Dailyfight"
		],
		"next": [
			"Dailyfight@visitCity"
		],
		"interrupt": [
			"inCity",
			"journey"
		]
	},

	"closeinfo@newsignincity@fight": {
		"recognition": "OCR",
		"expected": [
			"触碰空白区域退出",
			"任务完成",
			"客运收益结算",
			"公告",
			"资讯"
		],
		"action": "Click",
		"target": [
			100,
			661,
			105,
			19
		],
		"next": [
			"closeinfo@newsignincity@fight",
			"DailyFight"
		]
	},

	"home@Dailyfight": {
		"recognition": "TemplateMatch",
		"template": "home.png",
		"action": "Click",
		"next": "Dailyfight@visitCity"
	},

	"Dailyfight@visitCity": {
		"recognition": "OCR",
		"expected": "访问城市",
		"action": "Click",
		"next": "Dailyfight",
		"interrupt": [
			"home",
			"DailyFight"
		]
	},

	"Dailyfight": {
		"post_delay": 2000,
		"next": [
			"Bureau",
			"BureauUpright",
			"BureauDownleft"
		],
		"interrupt": [
			"home",
			"visitCity"
		]
	},

	"Bureau": {
		"recognition": "OCR",
		"expected": "铁安局",
		"action": "Click",
		"target_offset": [
			0,
			50,
			0,
			0
		],
		"post_delay": 1200,
		"next": "bountymission",
		"interrupt": [
			"Bureaulocked"
		]
	},
	"Bureaulocked": {
		"recognition": "OCR",
		"expected": [
			"负责人不在",
			"先去其他地方逛逛吧"
		],
		"next": "home@StopTask"
	},

	"BureauUpright": {
		"post_delay": 500,
		"next": [
			"bountymission",
			"Bureau"
		],
		"interrupt": [
			"seekUpright"
		]
	},

	"BureauDownleft": {
		"post_delay": 500,
		"next": [
			"bountymission",
			"Bureau"
		],
		"interrupt": [
			"seekDownleft"
		]
	},

	"bountymission": {
		"recognition": "OCR",
		"expected": [
			"悬赏任务"
		],
		"action": "Click",
		"post_delay": 1500,
		"next": [
			"missionnum-0"
		],
		"interrupt": [
			"bountylevel5",
			"bountylevel4",
			"bountylevel3",
			"bountylevel2",
			"bountylevel1"
		]
	},

	"bountylevel5": {
		"recognition": "TemplateMatch",
		"template": "Fight/5stars.png",
		"threshold": 0.95,
		"next": "gofightingsmall",
		"interrupt": "deleteimage"
	},

	"bountylevel4": {
		"recognition": "TemplateMatch",
		"template": "Fight/4star.png",
		"threshold": 0.95,
		"next": "deleteimage"
	},

	"bountylevel3": {
		"recognition": "TemplateMatch",
		"template": "Fight/3star.png",
		"threshold": 0.95,
		"next": "deleteimage"
	},

	"bountylevel2": {
		"recognition": "TemplateMatch",
		"template": "Fight/2star.png",
		"threshold": 0.95,
		"next": "deleteimage"
	},

	"bountylevel1": {
		"recognition": "TemplateMatch",
		"template": "Fight/1star.png",
		"threshold": 0.95,
		"next": "deleteimage"
	},

	"gofightingsmall": {
		"recognition": "TemplateMatch",
		"template": "Fight/gofightingsmall.png",
		"action": "Click",
		"post_delay": 1500,
		"next": "gofightinglarge-20"
	},

	"gofightinglarge-20": {
		"recognition": "TemplateMatch",
		"template": "Fight/gofightinglarge-20.png",
		"action": "Click",
		"post_delay": 1500,
		"next": [
			"autofighting",
			"victory",
			"defeat"
		],
		"interrupt": "autofightclosing"
	},

	"pasuefighting": {
		"recognition": "TemplateMatch",
		"template": "Fight/pause.png",
		"post_delay": 2000
	},

	"autofighting": {
		"recognition": "FeatureMatch",
		"template": [
			"Fight/autofight.png",
			"Fight/autofight1.png"
		],
		"post_wait_freezes": 30000,
		"rate_limit": 8000,
		"next": "fightingend"
	},

	"autofightclosing": {
		"recognition": "TemplateMatch",
		"template": "Fight/autofightclose.png",
		"action": "Click"
	},

	"fightingend": {
		"post_wait_freezes": 2000,
		"next": [
			"victory",
			"defeat",
			"autofighting"
		]
	},

	"victory": {
		"recognition": "TemplateMatch",
		"template": "Fight/victory.png",
		"next": "nextstep"
	},

	"defeat": {
		"recognition": "TemplateMatch",
		"template": "Fight/defeat.png",
		"next": "nextstep"
	},

	"nextstep": {
		"recognition": "TemplateMatch",
		"template": "Fight/nextstep.png",
		"action": "Click",
		"post_delay": 1000,
		"rate_limit": 2000,
		"next": "bountymission"
	},

	"delmission": {
		"next": [
			"confirm"
		]
	},
	"deleteimage": {
		"recognition": "TemplateMatch",
		"template": "Fight/delete.png",
		"action": "Click",
		"next": [
			"confirmshort"
		]
	},

	"confirmshort": {
		"recognition": "TemplateMatch",
		"template": "confirmshort.png",
		"action": "Click"
	},

	"delowstarmission": {
		"action": "Click",
		"target": "bountylevel2",
		"next": [
			"delmission"
		]
	},

	"missionnum-0": {
		"recognition": "OCR",
		"pre_delay": 800,
		"expected": "暂无城市悬赏任务",
		"roi": [
			620,
			270,
			300,
			220
		],
		"doc": "home@store,使用plcli集成任务",
		"next": [
			"home@StopTask"
		]
	},

	"home@store": {
		"recognition": "TemplateMatch",
		"template": "home.png",
		"action": "Click",
		"post_delay": 1000,
		"next": "DailyRest"
	},

	"home@StopTask": {
		"recognition": "TemplateMatch",
		"template": "home.png",
		"action": "Click",
		"next": "stopnow"
	}
}
